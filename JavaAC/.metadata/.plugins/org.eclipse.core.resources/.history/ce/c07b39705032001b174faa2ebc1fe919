import java.awt.AWTException;
import java.awt.Robot;
import java.awt.event.KeyEvent;
import java.util.Random;
import java.util.concurrent.TimeUnit;

public class AutoPilot {
	static int n = 0;
	
	public static void StartAutoPilot() throws InterruptedException, AWTException {
		for(int i = 0; i < Gui.Waves; i++) {
			
			
			Thread.sleep(1500);
			 AutoPilot.LClick();
			 Thread.sleep(500);
			 
			 if(n == 0) {
				 AutoPilot.Spam1();	 
				 n = 1;
			 } 
			 else if(n == 1) {
				 AutoPilot.Spam2(); 
				 n = 0;
			 }
			 
			 
			 
			 
			 AutoPilot.Enter();			
			 Thread.sleep((Gui.Delay * 1000) + 1000);			
			 AutoPilot.LClick();
			 Thread.sleep(500);	
			 
			 if(n == 0) {
				 AutoPilot.Spam4();	 
				 n = 1;
			 } 
			 else if(n == 1) {
				 AutoPilot.Spam3();	
				 n = 0;
			 }
			 
					 			 
			 AutoPilot.Enter();
			 Thread.sleep((Gui.Delay * 1000) + 1000);	
		}
		
		
	}
	
	
	

